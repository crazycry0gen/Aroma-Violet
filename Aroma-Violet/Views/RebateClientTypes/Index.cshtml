@model IEnumerable<Aroma_Violet.Models.RebateClientType>

@{
    ViewBag.Title = "Index";
}

<h2>Rebate Structures</h2>

<p>
    @Html.ActionLink("Create New Rebate Structure", "Create")|
    @*@Html.ActionLink("Calc Subscription", "CalculateSubscriptionRebates")|
    @Html.ActionLink("Calc Group", "CalculateGroupSalesRebates")|
    @Html.ActionLink("Month End", "CalculateMonthEnd")*@
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ClientTypeID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ClientType.ClientTypeName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.RebateClientTypeId }) |
            @Html.ActionLink("Copy", "Copy", new { sourceRebateClientTypeID = item.RebateClientTypeId })
           
        </td>
    </tr>
}

</table>
