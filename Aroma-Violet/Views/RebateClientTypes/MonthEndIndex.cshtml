@model int[]

<table style="min-height:850px">
    <tr>
        <td>
            <table>
                <tr>
                    <th><h2>Period</h2></th>
                </tr>
                @for (int i = 0; i < Model.Length; i++)
            {
                    <tr>
                        <td>@Html.ActionLink(@Model[i].ToString(), "ViewMonthEnd", new { periodId = Model[i] })</td>
                    </tr>
                }

                <tr></tr>
            </table>
        </td>
    </tr>
    <tr></tr>
</table>
